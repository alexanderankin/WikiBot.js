#!/usr/bin/env node
var commander = require('commander');

async function category(argv = process.argv) {
  var program = new commander.Command();
  program.name('wikibot-category');

  if (argv.length === 2) {
    program.outputHelp();
    process.exit(0);
  }

  program.parse(argv);
}


if (require.main === module) {
  category(process.argv).then(() => {});
}

module.exports = category;
